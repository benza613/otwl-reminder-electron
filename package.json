{
  "name": "otwl",
  "version": "1.0.0",
  "description": "Test app for benito",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"CryptoApp\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "postinstall": "electron-builder install-app-deps",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "com.electron.benappid",
    "productName": "Electron_Crpto",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "win": {
      "target": "nsis",
      "icon": "assets/icons/win/icon.ico"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "2.0.0",
    "electron-builder": "^20.11.1",
    "electron-packager": "^12.0.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "electron-push-receiver": "^2.0.0",
    "electron-settings": "^3.2.0"
  }
}
